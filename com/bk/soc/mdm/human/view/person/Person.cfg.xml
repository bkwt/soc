<?xml version="1.0" encoding="UTF-8"?>
<AppConfig>
	<Area id="mainArea" type="TreeAreaUnit">
		<Case value="EDIT">
			<Edit>true</Edit>
			<SelectAble>true</SelectAble>
			<Title>人员维护</Title>
               <Operaters>
                 <DbClickOperater>
					<Method>openUpdateWindow</Method>
					<ConstParam></ConstParam>
				</DbClickOperater>
				<Operater>
					<Text>新增</Text>
					<Icon>/SOC/org/fdm/images/button/new1.gif</Icon>
					<Method>openAddWindow</Method>
					<ConstParam>ADDNEW</ConstParam>
				</Operater>	
				<Operater>
					<Text>修改</Text>
					<Icon>/SOC/org/fdm/images/button/update1.gif</Icon>
					<Method>openUpdateWindow</Method>
					<ConstParam>UPDATE</ConstParam>
				</Operater>				
				<Operater>
					<Text>删除</Text>
					<Icon>/SOC/org/fdm/images/button/delete1.gif</Icon>
					<Method>opDelete</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
			<!--
			<Controller>
	            <Function>controlCH</Function>
            </Controller>
		--></Case>		
		<Case value="VIEW">
			<Edit>false</Edit>
			<SelectAble>false</SelectAble>
			<Title>人员查询</Title>
			<Operaters>				
			</Operaters>
		</Case>
	</Area>	
	
	<Area id="detailArea" type="DetailAreaUnit">
		<Case value="ADDNEW">
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Icon>/SOC/org/fdm/images/button/save1.gif</Icon>
					<Method>opInsert</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>保存关闭</Text>
					<Icon>/SOC/org/fdm/images/button/save1.gif</Icon>
					<Method>opInsertAndClose</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
		</Case>
		<Case value="UPDATE">
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Icon>/SOC/org/fdm/images/button/save1.gif</Icon>
					<Method>opUpdate</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
		</Case>
		
	</Area>		
	
</AppConfig>