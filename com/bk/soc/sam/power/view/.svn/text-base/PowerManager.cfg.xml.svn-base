	<Area id="detailMethodArea" type="DetailAreaUnit">
		<Case value="ADDNEW">
			<Edit>true</Edit>
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Method>opSaveQueryPowers</Method>
