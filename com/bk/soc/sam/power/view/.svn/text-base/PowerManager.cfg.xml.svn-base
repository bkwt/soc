					<Method>backToMain</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
		</Case>
		<Case value="EDIT">
			<Edit>true</Edit>
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Method>opSaveQueryPowers</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>返回</Text>
					<Method>backToMain</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
			<Fields>
				<Field name="methodName" xtype="textfield">
					<Enabled>false</Enabled>
					<Default></Default>
				</Field>
			</Fields>
