<?xml version="1.0" encoding="UTF-8"?>
<AppConfig>
	<Area id="mainArea" type="TreeAreaUnit">
		<Case value="EDIT">
			<Edit>true</Edit>
			<SelectAble>true</SelectAble>
			<Operaters>
				<Operater>
					<Text>新增</Text>
					<Icon>/SOC/org/fdm/images/button/new1.gif</Icon>
					<Method>openAddWindow</Method>
					<ConstParam>ADDNEW</ConstParam>
				</Operater>
				
								
				<Operater>
					<Text>保存</Text>
					<Icon>/SOC/org/fdm/images/button/save1.gif</Icon>
					<Method>opUpdate</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>删除</Text>
					<Icon>/SOC/org/fdm/images/button/delete1.gif</Icon>
					<Method>opDelete</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>新增分组</Text>
					<Icon>/SOC/org/fdm/images/button/new1.gif</Icon>
					<Method>openaddAdlist</Method>
					<ConstParam>ADDNEWLIST</ConstParam>
				</Operater>
				<Operater>
					<Text>删除分组</Text>
					<Icon>/SOC/org/fdm/images/button/delete1.gif</Icon>
					<Method>opDeleteAdlist</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters><!--
			<Controller>
	            <Function>controlCH</Function>
            </Controller>
		--></Case>		
		<Case value="VIEW">
			<Edit>false</Edit>
			<SelectAble>false</SelectAble>
			<Title>知识查询</Title>
			<Operaters>				
			</Operaters>
		</Case>
	</Area>	
	
	<Area id="detailArea" type="DetailAreaUnit">
		<Case value="ADDNEW">
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Method>opInsert</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>保存关闭</Text>
					<Method>opInsertAndClose</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
			<Fields>
			</Fields>
		</Case>
	</Area>	
	<Area id="detailArea1" type="DetailAreaUnit">
		<Case value="ADDNEWLIST">
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Method>opListInsert</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>保存关闭</Text>
					<Method>opListInsertAndClose</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
			<Fields>
			</Fields>
		</Case>
	</Area>	
	<Area id="detailArea2" type="DetailAreaUnit">
		<Case value="REMOVE">
			<Operaters>
				<Operater>
					<Text>保存</Text>
					<Method>opRemoveInsert</Method>
					<ConstParam></ConstParam>
				</Operater>
				<Operater>
					<Text>保存关闭</Text>
					<Method>opRemoveInsertAndClose</Method>
					<ConstParam></ConstParam>
				</Operater>
			</Operaters>
			<Fields>
			</Fields>
		</Case>
	</Area>			
	
</AppConfig>